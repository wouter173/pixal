  "bugs": {
    "url": "https://github.com/wouter173/pixal/issues"
  },
  "homepage": "https://github.com/wouter173/pixal#readme",
  "devDependencies": {
    "@typescript-eslint/parser": "^2.13.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^6.8.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "discord.js": "^11.5.1"
  }
}
