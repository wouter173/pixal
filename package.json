{
  "name": "pixal",
  "files": [
    "lib/**/*"
  ],
  "version": "1.0.0",
  "description": "A simple discord bot library.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && npm run bundle",
